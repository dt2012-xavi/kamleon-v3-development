        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KamleonApp"
        tools:targetApi="31">
        <activity
            android:name=".activities.AnalyticActivity"
            android:exported="false" />
        <activity
            android:name="com.dynatech2012.kamleonuserapp.activities.EmailActivity"
            android:exported="false" />
        <activity
            android:name="com.dynatech2012.kamleonuserapp.activities.PwdActivity"
