            android:name="com.dynatech2012.kamleonuserapp.activities.PrivacyActivity"
            android:exported="false" />
        <activity
            android:name="com.dynatech2012.kamleonuserapp.activities.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".activities.InitActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.GraphicsActivity"
            android:exported="false" />
    </application>

</manifest>