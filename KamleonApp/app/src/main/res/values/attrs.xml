<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KamleonInputBoxStyle">
        <attr name="placeholder" format="string" />
        <attr name="secureInput" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SettingMenuItemViewStyle">
        <attr name="settingTypeLabel" format="string" />
        <attr name="settingTypeValue" format="string" />
        <attr name="settingRightArrow" format="boolean" />
        <attr name="settingRightSwitch" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AnalyticListItemViewStyle">
        <attr name="typeText" format="string" />
        <attr name="titleText" format="string" />
        <attr name="descText" format="string" />
        <attr name="type" format="integer" />
    </declare-styleable>

    <declare-styleable name="NotificationListItemViewStyle">
        <attr name="notiType" format="string" />
        <attr name="notiDate" format="string" />
        <attr name="notiDesc" format="string" />
        <attr name="notiAction" format="string" />
    </declare-styleable>

    <declare-styleable name="AnalyticsComposeItemViewStyle">
        <attr name="analyticsType" format="enum" >
            <enum name="hydration" value="0"/>
            <enum name="electrolytes" value="1"/>
            <enum name="volume" value="2"/>
        </attr>
    </declare-styleable>
</resources>